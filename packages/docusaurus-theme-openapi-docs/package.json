{
  "name": "docusaurus-theme-openapi-docs",
  "description": "OpenAPI theme for Docusaurus.",
  "version": "4.5.1",
  "license": "MIT",
  "keywords": [
    "openapi",
    "documentation",
    "docusaurus",
    "websites",
    "theme"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PaloAltoNetworks/docusaurus-openapi-docs.git",
    "directory": "packages/docusaurus-theme-openapi-docs"
  },
  "bugs": {
    "url": "https://github.com/PaloAltoNetworks/docusaurus-openapi-docs/issues"
  },
  "types": "src/theme-openapi.d.ts",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc --build && node ../../scripts/copyUntypedFiles.mjs && prettier --config ../../.prettierrc.json --write \"lib/theme/**/*.js\"",
    "watch": "concurrently --names \"lib,lib-next,tsc\" --kill-others \"yarn babel:lib --watch\" \"yarn babel:lib-next --watch\" \"yarn tsc --watch\""
  },
  "devDependencies": {
    "@docusaurus/theme-classic": "^3.5.0",
    "@docusaurus/theme-common": "^3.5.0",
    "@docusaurus/types": "^3.5.0",
    "@types/crypto-js": "^4.2.2",
    "@types/file-saver": "^2.0.7",
    "@types/lodash": "^4.17.20",
    "@types/pako": "^2.0.3",
    "@types/postman-collection": "^3.5.11",
    "@types/react-modal": "^3.16.3",
    "concurrently": "^9.2.0",
    "docusaurus-plugin-openapi-docs": "^4.5.1",
    "docusaurus-plugin-sass": "^0.2.6",
    "eslint-plugin-prettier": "^5.5.1"
  },
  "dependencies": {
    "@hookform/error-message": "^2.0.1",
    "@reduxjs/toolkit": "^2.8.2",
    "allof-merge": "^0.6.6",
    "buffer": "^6.0.3",
    "clsx": "^2.1.1",
    "copy-text-to-clipboard": "^3.2.0",
    "crypto-js": "^4.2.0",
    "file-saver": "^2.0.5",
    "lodash": "^4.17.21",
    "pako": "^2.1.0",
    "postman-collection": "^5.0.2",
    "httpsnippet-lite": "^3.0.5",
    "@har-sdk/postman": "^2.4.7",
    "@har-sdk/core": "^1.5.0",
    "path-browserify": "^1.0.1",
    "prism-react-renderer": "^2.4.1",
    "process": "^0.11.10",
    "react-hook-form": "^7.59.0",
    "react-live": "^4.1.8",
    "react-magic-dropzone": "^1.0.1",
    "react-markdown": "^10.1.0",
    "react-modal": "^3.16.3",
    "react-redux": "^9.2.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "4.0.1",
    "sass": "^1.89.2",
    "sass-loader": "^16.0.5",
    "unist-util-visit": "^5.0.0",
    "url": "^0.11.4",
    "xml-formatter": "^3.6.6"
  },
  "peerDependencies": {
    "@docusaurus/theme-common": "^3.5.0",
    "docusaurus-plugin-openapi-docs": "^4.0.0",
    "docusaurus-plugin-sass": "^0.2.3",
    "react": "^16.8.4 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^16.8.4 || ^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "engines": {
    "node": ">=14"
  }
}
